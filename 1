    function moedas()
        while _G.moedas do 
            local args = {
                [3] = game:GetService("Players").LocalPlayer
            }

            game:GetService("ReplicatedStorage").Remote.Event.RebirthShop:FindFirstChild("[C-S]TryExchangeStar"):FireServer(unpack(args))
            wait(5)
        end
    end

    function spins()
        while _G.spins do 
            game:GetService("ReplicatedStorage").Remote.Function.Spin:FindFirstChild("[C-S]TrySpin"):InvokeServer()
            wait(0.01)
        end
    end 

    function craft()
        while _G.craft do 
