local eggOptions = {}  -- Armazenar opções de ovos para o dropdown

for eggName, _ in pairs(EggScripts) do
    table.insert(eggOptions, eggName)  -- Adicionar o nome do ovo às opções do dropdown
end

EGGSTab:AddDropdown({
    Name = "Select Egg",
    Default = "EVENTOEGG",
    Options = {"EVENTOEGG", "EGG1", "EGG2", "EGG3", "EGG4", "EGG5", "EGG6", "EGG7", "EGG8", "EGG9", "EGG10", "EGG11",
    "EGG12", "EGG13", "EGG14", "EGG15", "EGG16", "EGG17", "EGG18", "EGG19", "EGG20", "EGG21", "EGG22", "EGG23", "EGG24", 
       "EGG25", "EGG26", "EGG27", "EGG28", "EGG29", "EGG30"}, 
     
    Callback = function(Value)
        eggNumber = Value
    end
})

EGGSTab:AddToggle({
    Name = "Open Eggs",
    Default = false,
    Callback = function(Value)
        if Value then
            openingEgg = true
            while openingEgg do
                local eggScript = EggScripts[eggNumber]

                if eggScript then
                    eggScript()
                end

                wait(0.001)
            end
        else
            openingEgg = false
        end
    end
})
local Section = EGGSTab:AddSection({
    Name = "Craft All"
})

EGGSTab:AddToggle({
    Name = "Craft All",
    Default = false,
    Callback = function(Value)
        _G.craft = Value
        craft()
    end    
})

